<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafxdemo.controller.StartpageLoggedInController">
    <right>
        <AnchorPane prefHeight="206.0" prefWidth="266.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="30.0" right="5.0" top="20.0" />
            </BorderPane.margin>
            <Button fx:id="startPageLoggedInLogOutButton" layoutX="153.0" layoutY="176.0" mnemonicParsing="false" onAction="#onStartPageLoggedInLogOutButton" text="Log out" />
        </AnchorPane>
    </right>
    <top>
        <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="10.0" left="20.0" right="20.0" top="20.0" />
            </padding>
         <Label fx:id="startPageLoggedInWelcomeLabel" prefHeight="17.0" prefWidth="234.0" />
            <Button fx:id="startPageLoggedInSearchButton" mnemonicParsing="false" onAction="#onStartPageLoggedInSearchButtonClick" text="Search" />
         <Button fx:id="startPageLoggedInActiveLoansButton" mnemonicParsing="false" onAction="#onStartPageLoggedInActiveLoansButton" text="Active Loans" />
        </HBox>
    </top>
    <left>
        <ImageView fitHeight="127.0" fitWidth="211.0" layoutX="20.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
            <effect>
                <Shadow />
            </effect>
            <BorderPane.margin>
                <Insets left="5.0" />
            </BorderPane.margin>
            <Image url="@../images/UserLoginIcon.jpg" />
        </ImageView>
    </left>
    <padding>
        <Insets left="5.0" right="5.0" />
    </padding>
</BorderPane>
